# .env.development
# Application
PORT=3000
NODE_ENV=development

# Database
DB_TYPE=postgres
DB_HOST=localhost
DB_PORT=5432
DB_USERNAME=postgres
DB_PASSWORD=postgres
DB_DATABASE=nest_db
DB_SSL=false

# JWT
JWT_SECRET=your-super-secret-key-here
JWT_EXPIRATION=1h
JWT_REFRESH_EXPIRATION=7d

# CORS
CORS_ORIGIN=http://localhost:3000,http://localhost:4200

# Cache
CACHE_TTL=3600
CACHE_MAX_ITEMS=1000

# Logging
LOG_LEVEL=debug

# Security
RATE_LIMIT_TTL=60
RATE_LIMIT_MAX=100